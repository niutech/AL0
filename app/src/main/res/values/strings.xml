    <!-- MainActivity Intro Step 1 -->
    <string name="main_activity_intro_step1_title">1/4 – Welcome.</string>
    <string name="main_activity_intro_step1_body">AL0 is an application launcher designed to declutter your smartphone.
        \n\nFollow the next screens to setup and use AL0.
    </string>
    <string name="main_activity_intro_step1_continue_button">Continue</string>
    <!-- MainActivity Intro Step 2 -->
    <string name="main_activity_intro_step2_title">2/4 – A quiet tool.</string>
    <string name="main_activity_intro_step2_body">Notifications are annoying, addictive and invasive. AL0 uses Notification access to quiet them.
        \n\nDo you want to grant Notification access to AL0?
    </string>
    <string name="main_activity_intro_step2_yes_button">Yes</string>
    <string name="main_activity_intro_step2_no_button">No</string>
    <!-- MainActivity Intro Step 3 -->
    <string name="main_activity_intro_step3_title">3/4 – Your phone.</string>
    <string name="main_activity_intro_step3_body">AL0 turns your smartphone into a minimalist device that supports phone calls, sms and a few other features.
        \n\nTo make AL0 works correctly it must be your default phone and messaging app.
        \n\nDo you want AL0 to be the default phone and messaging app?
    </string>
    <string name="main_activity_intro_step3_yes_button">Yes</string>
    <string name="main_activity_intro_step3_no_button">No</string>
    <!-- MainActivity Intro Step 4 -->
    <string name="main_activity_intro_step4_title">4/4 – Essential mode.</string>
    <string name="main_activity_intro_step4_body">AL0 do one thing at a time, activate pin screen to keep external running apps from interfering your experience.
        \n\nDo you want to pin screen AL0?
    </string>
    <string name="main_activity_intro_step4_yes_button">Yes</string>
    <string name="main_activity_intro_step4_no_button">No</string>

