<resources>
    <!-- Base application theme. -->
    <!--
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    -->
    <style name="AppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:background">@color/color_5</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>

        <item name="android:statusBarColor">@color/color_5_alpha0</item>
        <item name="android:navigationBarColor">@color/color_5_alpha0</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>


        <!--
        <item name="android:windowContentOverlay">@null</item>
        -->
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowBackground">@color/color_5</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

    <!--
         COMPONENTS
         __________
    -->

    <!-- BUTTON -->
    <style name="button" parent="typography_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

        <!--
        <item name="android:layout_marginTop">-3dp</item>
        <item name="android:layout_marginBottom">-3dp</item>
        -->
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@drawable/button_background</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:minWidth">24dp</item>
        <!--
        <item name="android:layout_marginLeft">-3dp</item>
        <item name="android:layout_marginRight">-3dp</item>
        -->
    </style>

    <style name="button_full_width" parent="button">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="button_fill" parent="button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="button_single_char" parent="button">
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
    </style>

    <style name="button_single_char_wide" parent="button_single_char">
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:background">@color/color_5_alpha0</item>
    </style>

    <style name="button_back" parent="button_single_char_wide">
    </style>

    <style name="button_squared" parent="button">
        <item name="android:minWidth">40dp</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- TEXT INPUT -->
    <style name="text_input" parent="typography_text_input">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
    </style>

    <style name="text_input_centered" parent="text_input">
        <item name="android:gravity">center</item>
    </style>

    <style name="text_input_right" parent="text_input">
        <item name="android:gravity">right</item>
    </style>

    <!-- TITLE -->
    <style name="title" parent="typography_base">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
    </style>


    <!-- LIST ITEM -->
    <style name="list_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
